/* pages/usecar/usercar.wxss */

#mymap{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 10
}

.map_btn{
  position:fixed;
  bottom: 0;
  z-index: 20;
  display: flex;
  justify-content: space-around;
  width: 100%;
  padding-bottom:40rpx;
}
.map_btn button{
  width:160rpx;
  height:160rpx;
  border-radius: 100%;
  font-size: 25rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: #fff;
  box-shadow: 0px 2px 25px -1px rgba(0,0,0,0.25) 
}
.suoche{
  background: #FFB68B
}
.huanche{
  background: #FF7173
}
.icon{
  width: 50rpx;
  height: 50rpx;
}