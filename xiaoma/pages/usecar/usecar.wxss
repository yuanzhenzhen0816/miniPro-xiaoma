/* pages/usecar/usercar.wxss */
@import '../common/common.wxss';
.mymap{
  width: 100%;
  height:550rpx;
  position: absolute;
  left: 0;
  z-index: 100;
  top:310rpx;
}

.map_btn{
  position:absolute;
  display: flex;
  justify-content: space-around;
  width: 100%;
  padding-bottom:130rpx;
  z-index: 300;
  bottom: 0
}
.map_btn view{
  width:180rpx;
  height:180rpx;
  border-radius: 100%;
  border: none;
  font-size: 12rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: #fff;
  box-shadow: 0px 2px 25px -1px rgba(0,0,0,0.25) 
}
.suoche{
  background: #FFB68B
}
.huanche{
  background: #FF7173
}
.icon{
  width: 50rpx;
  height: 50rpx;
}